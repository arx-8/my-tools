{
  "name": "my-tools",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.10",
    "emotion-normalize": "10.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "typescript": "3.5.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.tsx > eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx src",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
