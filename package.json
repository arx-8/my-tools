{
  "name": "my-tools",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "7.2.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/node": "12.12.26",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-typescript-sort-keys": "0.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-scripts": "3.3.1",
    "typescript": "3.7.5"
  },
  "scripts": {
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js src",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
