{
  "name": "my-tools",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.54",
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "3.2.1",
    "@testing-library/user-event": "10.4.0",
    "autotrack": "2.4.1",
    "csv-tools": "1.1.1",
    "diff": "4.0.2",
    "emotion-normalize": "10.1.0",
    "immer": "6.0.9",
    "json-url": "2.6.0",
    "lodash": "4.17.15",
    "match-sorter": "4.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "2.7.0",
    "react-helmet": "6.0.0",
    "react-number-format": "4.4.1",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.13.1",
    "react-use": "15.1.1",
    "ulid": "2.3.0",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@types/diff": "4.0.2",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.154",
    "@types/match-sorter": "4.0.0",
    "@types/node": "14.0.6",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/react-router-dom": "5.1.5",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-typescript-sort-keys": "1.2.0",
    "import-sort-style-module": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "react-scripts": "3.4.1",
    "source-map-explorer": "2.4.2",
    "typescript": "3.9.3"
  },
  "scripts": {
    "analyze:run": "source-map-explorer 'build/static/js/*.js' --html misc/source-map-explorer.html",
    "analyze": "npm-run-all --sequential build analyze:run",
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js src",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "update-all": "npx npm-check-updates -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
